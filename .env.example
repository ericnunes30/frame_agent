# .env.example - Exemplo de variáveis de ambiente para o Construtor de Agentes

# Ambiente
NODE_ENV=development

# Chave de API para OpenAI (obrigatória para providers OpenAI)
OPENAI_API_KEY=sua_chave_api_aqui

# URL base para APIs compatíveis com OpenAI (ex: LocalAI, Ollama, etc.)
OPENAI_BASE_URL=http://localhost:8080/v1

# Provider de LLM a ser usado (openai-compatible, openai, anthropic, etc.)
LLM_PROVIDER=openai-compatible

# Modelo a ser usado para os agentes
MODEL=gpt-4o-mini

# Configurações de retry e timeout
MAX_RETRIES=3
REQUEST_TIMEOUT=30000

# Chave de API para Anthropic (obrigatória se usar provider anthropic)
ANTHROPIC_API_KEY=sua_chave_api_anthropic_aqui

# Chave de API para Google (obrigatória se usar provider google)
GOOGLE_API_KEY=sua_chave_api_google_aqui